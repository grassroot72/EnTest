<?xml version='1.0' encoding='UTF-8' standalone='yes' ?>
<questions>

   <question type="TF" id="1">
      <content key="( F )">A young man is travelling in the south of a country.</content>
      <choice id="T">True</choice>
      <choice id="F">False</choice>
   </question>

   <question type="TF" id="2">
      <content key="( T )">On his right is a river, and on his left is the thick, dark forest.</content>
      <choice id="T">True</choice>
      <choice id="F">False</choice>
   </question>

   <question type="TF" id="3">
      <content key="( F )">He sees a big crocodile looking at him with two eyes.</content>
      <choice id="T">True</choice>
      <choice id="F">False</choice>
   </question>

   <question type="TF" id="4">
      <content key="( F )">The lion on his right is trying to jump on him.</content>
      <choice id="T">True</choice>
      <choice id="F">False</choice>
   </question>

   <question type="TF" id="5">
      <content key="( T )">He can't save his life by swimming.</content>
      <choice id="T">True</choice>
      <choice id="F">False</choice>
   </question>

   <question type="TF" id="6">
      <content key="( T )">He is too frightened to open his eyes.</content>
      <choice id="T">True</choice>
      <choice id="F">False</choice>
   </question>

   <question type="TF" id="7">
      <content key="( T )">He hears something moving as the lion jumps.</content>
      <choice id="T">True</choice>
      <choice id="F">False</choice>
   </question>

   <question type="TF" id="8">
      <content key="( T )">They young man sees nothing as the lion jumps, because he closes his eyes.</content>
      <choice id="T">True</choice>
      <choice id="F">False</choice>
   </question>

   <question type="TF" id="9">
      <content key="( F )">When he opens his eyes, he finds that the crocodile is in the mouth of the lion.</content>
      <choice id="T">True</choice>
      <choice id="F">False</choice>
   </question>

</questions>